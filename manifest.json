{
  "manifest_version": 3,
  "name": "f*ck overlays v3",
  "author": "Mark Muego",
  "description": "adds a context menu to delete overlays that prevent you from viewing a page",
  "version": "3.0.0",
  "icons": {
    "16": "icon16.png",
    "128": "icon128.png"
  },
  "permissions": [
    "contextMenus"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "fuckem.js"
      ]
    }
  ],
  "background": {
    "persistent": true,
    "scripts": [
      "fuckoverlaysbg.js"
    ]
  }
}